@model IEnumerable<PBMS_beta3A.Model.ListaDaManutencao>

<p> Última feita às :@DateTime.Now.ToString("HH:mm:ss") </p>

<table>
     <tr>
        <th>
            Site
        </th>
        <th>
            Provincía
        </th>
        <th>
            Município
        </th>
        <th>
            Cliente
        </th>
        <th>
            Observação
        </th>
        <th>
            Última Manutenção
        </th>
        <th>
            Próxima Manutenção
        </th>
        <th>
            Responsável
        </th>
        <th>
            Estado Actual
        </th>
        <th>
            Add Item
        </th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Site)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Provincia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Municipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Observacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataUltimaManutencao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataProximaManutencao)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Funcionario)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
           @Html.ActionLink("Requisitar Material", "RequistarMaterial", new { id = item.Site }) 
        </td>
    </tr>
}

</table>
